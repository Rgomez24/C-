#include <iostream> 
#include <string>
using namespace std;

int main()
{
	int n;
	


	cout << "How many dogs do you own?" << endl;

	cin >> n;

	string * dog = new string[n];

	cout << "Ok, enter their names!" << endl;
	for (int i = 0; i < n; i++){
		cin >> dog[i];
	}
	for (int i = 0; i < n; i++){


		cout << "The names you entered: " << dog[i] << endl;
	}
	return 0;
}
