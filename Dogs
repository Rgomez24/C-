#include <iostream> 
#include <string>
using namespace std;

int main()
{
	int n;
	string dogs;


	cout << "How many dogs do you own?" << endl;

	cin >> n;

	string * dog = new string[n];

	cout << "Ok, enter their names!" << endl;
	for (int i = 0; i < n; i++){
		cin >> dog[i];
		dogs = dog[n];

	}
	cout << "The names you entered: "<< dogs << endl;
	return 0;
}
