#include <iostream>
#include <string>
using namespace std;

class node
{
public:
	string data;
	node *next;
	node(string d);
	string newHead;


};

node::node(string d)
{
	data = d;
	next = NULL;
}

void print(node * current)
{
	while (current != NULL) {
		cout << current->data << endl;
		current = current->next;
	}
}

void append(node *&current, string target)
{
	node * temp = current;
	if (current == NULL)
	{
		current = new node(target);
		return;
	}
	while (temp->next != NULL)
		{
			temp = temp->next;
		}
		temp->next = new node(target);
}


int main()
{

	node *head = NULL;

	append(head, "Conquest");
	append(head, "war");
	append(head, "death");

	append(head, "famine");

	print(head);

	return 0;
}
